{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include":["./src/core.js", "./src/lib/", "./src/utils/"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "recurse":true,
        "destination": "./documentation/"
    }
}